generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int      @id @default(autoincrement())
  nome            String   @db.VarChar(100)
  sexo            String   @db.Char(10)
  idade           Int      @db.Int
  cpf             String   @db.Char(11)
  data_nascimento DateTime @db.DateTime()
}
